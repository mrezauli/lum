{
    "name": "mrezauli/lum",
    "description": "laravel user management",
    "keywords": [
        "mrezauli",
        "lum"
    ],
    "homepage": "https://github.com/mrezauli/lum",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "MD. Rezaul Islam",
        "email": "rezaul.cse.mbstu@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0",
        "spatie/laravel-permission": "^2.37|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mrezauli\\Lum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrezauli\\Lum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "Laravel User Management Routes": "Because of changing the route file please replace user_management.php if you want to update version 0.6.5 or less than 0.6.5",
        "Vue js master file": "Add vue folder in views resource if you want to update version 0.6.5 or less than 0.6.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrezauli\\Lum\\LaravelUserManagementProvider"
            ]
        }
    }
}